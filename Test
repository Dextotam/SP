//Program 1://

PROGRAM PG;

BEGIN VAR INT 2 AA, BB, XX, YY;

INPUT(AA);
INPUT(BB);

OUTPUT(AA + BB);
OUTPUT(AA - BB);
OUTPUT(AA * BB);
OUTPUT(AA DIV BB);
OUTPUT(AA MOD BB);

 (AA - BB) * 10 + (AA + BB) DIV 10 => XX;
 XX+XX MOD 10 => YY;

OUTPUT(XX);
OUTPUT(YY);

END      

//Program 2://

PROGRAM PG

BEGIN VAR INT_2 AA, BB, CC, MX;

INPUT(AA);
INPUT(BB);
INPUT(CC);

IF (AA==BB) && (AA==CC) && (BB==CC)
{1} ELSE {0};
OUTPUT({});

MX => IF (AA LT BB) && (AA LT CC) {AA}
         
       ELSE IF (BB LT AA) && (BB LT CC) {BB}
    
       ELSE {CC}

OUTPUT(MX);

OUTPUT(
    IF (AA = BB) && (AA = CC) && (BB = CC) 
	{1}
    ELSE
        {0}
);

OUTPUT(
    IF (AA GT 0) || (BB GT 0) || (CC GT 0) 
        {-1}
    ELSE
        {0}
);

OUTPUT(
    IF (AA LT (BB + CC)) 
       {10}
    ELSE
        {0}
);

END    

//Program 3://

PROGRAM PG

BEGIN VAR INT_2 AA, BB, II, JJ, XX;

INPUT(AA);
INPUT(BB);

AA => II;
REPEAT
    OUTPUT(II * II);
     II + 1 =>II;
UNTIL II > BB;

0 => XX;
1 => II;

REPEAT
    1 => JJ;
    REPEAT
         XX + 1 => XX;
         JJ + 1 => JJ;
    UNTIL JJ > BB;
   II + 1 => II;
UNTIL II > AA;

OUTPUT(XX);

END                                                                                                                                                                                                                                                                            